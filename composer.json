{
  "name": "myonlinestore/guzzle-authorization-middleware",
  "type": "library",
  "description": "Guzzle Authorization Middleware",
  "homepage": "https://github.com/MyOnlineStore/guzzle-authorization-middleware",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "MyOnlineStore\\GuzzleAuthorizationMiddleware\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MyOnlineStore\\GuzzleAuthorizationMiddleware\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": "^7.2",
    "ext-json": "*",
    "guzzlehttp/guzzle": "^6.5 || ^7.3",
    "lcobucci/jwt": "^3.4 || ^4.0",
    "psr/cache": "^1.0",
    "psr/http-factory": "^1.0",
    "psr/http-message": "^1.0",
    "psr/log": "^1.1"
  },
  "require-dev": {
    "myonlinestore/coding-standard": "^3.1",
    "phpunit/phpunit": "^8.5",
    "psalm/plugin-phpunit": "^0.15.1",
    "vimeo/psalm": "^4.7"
  },
  "config": {
    "vendor-dir": "vendor",
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true,
    "platform": {
      "php": "7.2.24"
    }
  }
}
