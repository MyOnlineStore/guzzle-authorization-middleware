{
  "name": "myonlinestore/guzzle-authorization-middleware",
  "type": "library",
  "description": "Guzzle Authorization Middleware",
  "homepage": "https://github.com/MyOnlineStore/guzzle-authorization-middleware",
  "license": "Proprietary",
  "autoload": {
    "psr-4": {
      "MyOnlineStore\\GuzzleAuthorizationMiddleware\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MyOnlineStore\\GuzzleAuthorizationMiddleware\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": "^7.0.0",
    "ext-json": "*",
    "guzzlehttp/guzzle": "^6.3",
    "http-interop/http-factory-diactoros": "^1.0",
    "lcobucci/jwt": "^3.2",
    "psr/cache": "^1.0",
    "psr/http-factory": "^1.0",
    "psr/http-message": "^1.0",
    "zendframework/zend-diactoros": "^1.8"
  },
  "require-dev": {
    "escapestudios/symfony2-coding-standard": "^3.4",
    "phpunit/phpunit": "^6.5",
    "squizlabs/php_codesniffer": "^3.3"
  },
  "config": {
    "vendor-dir": "vendor",
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true,
    "platform": {
      "php": "7.0.25"
    }
  }
}
